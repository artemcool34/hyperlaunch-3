<?xml version="1.0"?>
<configuration>
    <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <runtime>
    <!--This section allows us to tell the app to search the lib folder for dlls apart from the actual app folder-->
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="Lib" />
    </assemblyBinding>
  </runtime>
</configuration>
