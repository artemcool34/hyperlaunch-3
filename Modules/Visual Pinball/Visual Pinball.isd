<?xml version="1.0" encoding="UTF-8"?>
<INISCHEMA>
	<INIFILES>
		<INIFILE name="%ModuleName%" required="false">
			<INITYPE>Module</INITYPE>
			<SECTIONS>
				<SECTION name="Settings" required="false">
					<SECTIONTYPE>Global</SECTIONTYPE>
					<KEYS>
						<KEY name="EscClose" required="false" nullable="false">
							<KEYTYPE>Boolean</KEYTYPE>
							<DESCRIPTION>This fixes VP from crashing (in WinXP) on exit when using Esc as your exit key.</DESCRIPTION>
						</KEY>
						<KEY name="ShowDMD" required="false" nullable="false">
							<KEYTYPE>Boolean</KEYTYPE>
							<DESCRIPTION>Enables/Disables the DMD (Dot Matrix Display).</DESCRIPTION>
						</KEY>
						<KEY name="ValidateDMDLaunch" required="false" nullable="false">
							<KEYTYPE>Boolean</KEYTYPE>
							<DESCRIPTION>If table has a DMD, make sure it launched.</DESCRIPTION>
						</KEY>
						<KEY name="UpdateDefaultDMD" required="false" nullable="false">
							<KEYTYPE>Boolean</KEYTYPE>
							<DESCRIPTION>Set this to true if you want the script to set the default position of the DMD on next run.</DESCRIPTION>
						</KEY>
						<KEY name="DMDX" required="false" nullable="true" minLength="0">
							<KEYTYPE>Integer</KEYTYPE>
							<DESCRIPTION>Your default X position of the DMD</DESCRIPTION>
						</KEY>
						<KEY name="DMDY" required="false" nullable="true" minLength="0">
							<KEYTYPE>Integer</KEYTYPE>
							<DESCRIPTION>Your default Y position of the DMD</DESCRIPTION>
						</KEY>
						<KEY name="DMDWidth" required="false" nullable="true" minLength="1">
							<KEYTYPE>Integer</KEYTYPE>
							<DESCRIPTION>Your default DMD Width</DESCRIPTION>
						</KEY>
						<KEY name="DMDHeight" required="false" nullable="true" minLength="1">
							<KEYTYPE>Integer</KEYTYPE>
							<DESCRIPTION>Your default DMD HeightHeight</DESCRIPTION>
						</KEY>
					</KEYS>
				</SECTION>
				<SECTION name="%RomName%" required="false">
					<SECTIONTYPE>Rom</SECTIONTYPE>
					<KEYS>
						<KEY name="ValidateDMDLaunch" required="false" nullable="false">
							<KEYTYPE>Boolean</KEYTYPE>
							<DESCRIPTION>If table has a DMD, make sure it launched.</DESCRIPTION>
						</KEY>
					</KEYS>
				</SECTION>
			</SECTIONS>
		</INIFILE>
	</INIFILES>
</INISCHEMA>